    <script>
        // Fonction globale pour ajuster le fond gris - dÃ©finie avant le chargement de la page
        function adjustGrayBackground() {
            console.log('[DEBUG] adjustGrayBackground appelÃ©');
            const imageContainer = document.getElementById('imageContainer');
            const grayBackground = document.getElementById('grayBackground');
            const section = document.getElementById('accueil');
            
            console.log('[DEBUG] Ã‰lÃ©ments pour adjustGrayBackground:', {
                imageContainer: !!imageContainer,
                grayBackground: !!grayBackground,
                section: !!section
            });
            
            if (imageContainer && grayBackground && section) {
                const imageHeight = imageContainer.offsetHeight;
                const sectionTop = section.offsetTop;
                const imageBottom = imageContainer.offsetTop + imageHeight;
                const relativeBottom = imageBottom - sectionTop;
                grayBackground.style.height = relativeBottom + 'px';
                console.log('[DEBUG] Hauteur ajustÃ©e:', relativeBottom + 'px');
            }
        }
        
        // Ã‰galement disponible sur window pour Ãªtre sÃ»r
        window.adjustGrayBackground = adjustGrayBackground;
        console.log('[DEBUG] adjustGrayBackground dÃ©fini globalement');
    </script>
