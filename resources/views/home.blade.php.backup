            modals.forEach(function(modalId) {
                const modal = document.getElementById(modalId);
                if (modal) {
                    modal.addEventListener('click', function(e) {
                        if (e.target === modal) {
                            closeModal(modalId);
                        }
                    });
                }
            });
            
            // Close on Escape key
            document.addEventListener('keydown', function(e) {
                if (e.key === 'Escape') {
                    modals.forEach(function(modalId) {
                        const modal = document.getElementById(modalId);
                        if (modal && modal.classList.contains('active')) {
                            closeModal(modalId);
                        }
                    });
                }
            });
