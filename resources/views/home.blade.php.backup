        <script>
            // Animation automatique dÃ©sactivÃ©e - gÃ©rÃ©e par CSS

            function scrollTestimonials(direction) {
                const container = document.getElementById('testimonialsContainer');
                const scrollAmount = 400;
                
                if (direction === 'left') {
                    container.scrollBy({ left: -scrollAmount, behavior: 'smooth' });
                } else {
                    container.scrollBy({ left: scrollAmount, behavior: 'smooth' });
                }
            }

            function toggleFAQ(index, event) {
                // EmpÃªcher le comportement par dÃ©faut et la propagation
                if (event) {
                    event.preventDefault();
                    event.stopPropagation();
                }
                
                // EmpÃªcher l'exÃ©cution multiple simultanÃ©e
                if (window.faqToggling) return;
                window.faqToggling = true;
                
                // RÃ©cupÃ©rer les Ã©lÃ©ments du menu cliquÃ© AVANT toute modification
                const clickedContent = document.getElementById(`faq-content-${index}`);
                const clickedIcon = document.getElementById(`faq-icon-${index}`);
                
                if (!clickedContent || !clickedIcon) {
                    window.faqToggling = false;
                    return;
                }
                
                // VÃ©rifier si le menu cliquÃ© est actuellement ouvert AVANT de fermer quoi que ce soit
                const wasOpen = !clickedContent.classList.contains('hidden');
                
                // Fermer TOUS les menus dÃ©roulants d'abord (sans exception)
                for (let i = 1; i <= 8; i++) {
                    const content = document.getElementById(`faq-content-${i}`);
                    const icon = document.getElementById(`faq-icon-${i}`);
                    if (content && icon) {
                        content.classList.add('hidden');
                        // RÃ©initialiser les styles inline pour laisser le CSS gÃ©rer
                        content.style.maxHeight = '';
                        content.style.opacity = '';
                        icon.style.transform = 'rotate(0deg)';
                    }
                }
                
                // Puis, si le menu cliquÃ© Ã©tait fermÃ© au moment du clic, on l'ouvre maintenant
                if (!wasOpen) {
                    // Retirer la classe hidden et rÃ©initialiser les styles
                    clickedContent.classList.remove('hidden');
                    clickedContent.style.maxHeight = '';
                    clickedContent.style.opacity = '';
                    clickedIcon.style.transform = 'rotate(180deg)';
                }
                
                window.faqToggling = false;
            }

            // Appeler la fonction au chargement et au redimensionnement (la fonction est dÃ©jÃ  dÃ©finie dans le head)
            window.addEventListener('load', adjustGrayBackground);
            window.addEventListener('resize', adjustGrayBackground);
        </script>

